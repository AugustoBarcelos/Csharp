using System;

public class Program
{
	public static void Main()
	{
		Console.WriteLine("System which verify if a number is odd or even.");
		Console.WriteLine("Enter with a number to be verified:");
		int a= int.Parse(Console.ReadLine());
		
		if (a % 2 ==0){
			Console.WriteLine("Even");
		}
		else if(a % 2 != 0){
		Console.WriteLine("Odd");
		}
	}
}
